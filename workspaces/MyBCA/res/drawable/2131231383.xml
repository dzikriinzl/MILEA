<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#ffffffff" />
            <stroke android:width="1.0dip" android:color="#ffcfcfcf" />
            <corners android:radius="12.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#fff8fbfe" />
            <stroke android:width="1.0dip" android:color="#ff868e96" />
            <corners android:radius="12.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#fff9f9f9" />
            <stroke android:width="1.0dip" android:color="#ffbcbcbc" />
            <corners android:radius="12.0dip" />
        </shape>
    </item>
</selector>
